Kalendar 

while True:
    jyl=int(input(" jyl: "))
    if jyl<=int(0):
        print(" Kayta engiz ")
    else:
        break
while True:
    ay = int(input(" ay: "))
    if ay>12 or ay<=int(0):
        print(" Kayta engiz ")
    else:
        break

while True:
    kun = int(input(" kun: "))
    if ay==2 and jyl%4 == 0  :
        if kun > int(29) or kun<=int(0):
            print(" Kayta engiz ")
        else:
            break

    elif ay == 2 and jyl%4!=0  :
        if kun>28 or kun<=0:
            print(" Kayta engiz ")
        else:
            break

    elif ay == 4 or 6 or 9 or 11:
        if kun>30 or kun<=int(0):
            print(" Kayta engiz ")
        else:
            break


    elif ay== 1 or 3 or 5 or 7 or 8 or 10 or 11:
            if kun>31 or kun<=int(0):
                print(" Kayta engiz ")
            else:
                break
    else:
        break

while True:
    sag=int(input(" sag: "))
    if sag>=24 or sag<int(0):
        print(" Kayta engiz ")
    else:
        break
while True:
    min=int(input(" min: "))
    if min>=60 or min<int(0):
        print(" Kayta engiz ")
    else:
        break
while True:
    sec=int(input(" sec: "))
    if sec>=60 or sec<int(0):
        print(" Kayta engiz ")
    else:
        break
print("\n{}.{}.{}.{}:{}:{}".format(jyl,ay,kun,sag,min,sec))#

